{
  "name": "@mimas/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "yarn build && node dist/index.js",
    "dev": "tsnd -r tsconfig-paths/register --respawn --exit-child --transpile-only --ignore-watch node_modules src/index.ts",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "pino": "^7.9.2",
    "pino-pretty": "^7.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "babel-plugin-module-resolver": "^4.1.0",
    "prisma": "^3.11.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1"
  }
}
